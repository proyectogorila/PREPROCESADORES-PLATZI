-var titulo = "Titulo Principal Hola"
-var titulos =  ["Titulo Principal", "Subtitulo 1","Subtitulo 2", "Subtitulo 3"]
-var enlaces = ["Acerca","Noticias","Contacto"]
-var menus = ["ACERCA","NOTICIAS","CONTACTO"]
mixin caja(imagen,titulo,contenido)
    .caja
        .caja__imagen: img(src="images/"+imagen, alt="")
        .caja__contenido
            h3=titulo
            p=contenido